import re

def jiajian(s):
    r = re.compile(r'\d+|\+|-')
    f = re.findall(r, s)
    #处理连续的两个符号'++','--','+-','-+'
    for i in range(len(f)):
        if f[i] == '-':
            if f[i+1]=='-':
                f[i]='+'
                del f[i+1]
            if f[i+1] == '+':
                del f[i]
                
        if f[i] == '+':
            if f[i+1]=='+':
                del f[i+1]
            if f[i+1]=='-':
                del f[i]
                
    for j in range(len(f)):
        if f[j] =='-':
            f[j] = '+'
            f[j+1] = float(f[j+1])*-1
    
    for i in f:
        if i == '+':
            i = 0
        sum += float(i)
        
        
